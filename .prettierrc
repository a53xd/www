{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 1000,
	"htmlWhitespaceSensitivity": "strict",
	"quoteProps": "as-needed",
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		},
		{
			"files": "*.md",
			"options": {
				"parser": "markdown"
			}
		}
	]
}
